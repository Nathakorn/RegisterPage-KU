<div ng-controller="registerController">
     
  <div class="row">
        <div class="col-sm-12">
            <h3>Search subject</h3>
        </div>
  </div>  
  <div class="row" style="margin-top: 3%;">
        <div class="col-sm-5">
            <input type="text" class="form-control" ng-model="selectedSubject" bs-options="state for state in states" placeholder="Enter subject ID or name" bs-typeahead>
        </div>
  </div>
  <div class="row" style="margin-top: 3%;">
        <div class="col-sm-2">
            <h4>Select Section:</h4>
        </div>
        <div class="col-sm-1" style="margin-top:0.75%;">
            <select>
              <option>1</option>
              <option>2</option>
              <option>450</option>
              <option>451</option>
            </select>
        </div>
  </div>  
  <div class="row" style="margin-top: 3%;">
        <div class="col-sm-2" style="margin-top:0.5%;">
            <div class="btn-group" ng-model="button.radio" bs-radio-group>
              <label class="btn btn-default"><input type="radio" class="btn btn-default" value="0">Credit</label>
              <label class="btn btn-default"><input type="radio" class="btn btn-default" value="1">Audit</label>
            </div>
        </div>
        <div class="col-sm-2" style="margin-top: 1%;">
           <button>Submit</button>
        </div>

  </div>
 
<!-- 
<div ng-repeat="obj in course">
  {{obj}}
</div>
-->
</div>
